{
  "name": "custom-extension",
  "version": "0.0.0",
  "description": "jsreport studio extension starter kit",
  "keywords": [
    "jsreport"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "test": "standard",
    "start": "jsreport-studio-start --httpPort 5488",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "MIT",
  "dependencies": {},
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "eslint": "6.7.1",
    "eslint-config-standard-jsx": "8.1.0",
    "eslint-plugin-babel": "5.3.0",
    "in-publish": "2.0.0",
    "jsreport-studio-dev": "1.5.0",
    "standard": "14.3.1"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
